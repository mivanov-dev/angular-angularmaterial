{
  "name": "angular-angularmaterial",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "npx http-server ./dist/browser -c-1 -p 4200 -P http://localhost:3000",
    "build": "ng build",
    "test": "ng test",
    "test-headless": "ng test --watch=false --progress=false --browsers=HeadlessChrome",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-headless": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "e2e-docker": "ng e2e --protractor-config=e2e/protractor-docker.conf.js --host=angular",
    "dev:ssr": "ng run angular-angularmaterial:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run angular-angularmaterial:server:production",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "prerender": "ng run angular-angularmaterial:prerender",
    "preinstall": "npm i -g @angular/cli typescript tslint sass",
    "docker:deploy": "docker-compose -f \"docker-compose.yml\" up -d --build",
    "wmu": "webdriver-manager update"
  },
