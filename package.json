  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-headless": "ng test --watch=false --karmaConfig=karma-ci.conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
